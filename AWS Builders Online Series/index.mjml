<mjml owa="desktop">
  <mj-head>
    <mj-font
      name="Ubuntu"
      href="https://fonts.googleapis.com/css2?family=Ubuntu:ital,wght@0,300;0,500;0,700;1,400;1,700&display=swap"
    />
    <mj-font
      name="Roboto"
      href="https://fonts.googleapis.com/css2?family=Roboto"
    />
    <mj-attributes>
      <!-- Default Section's Design  -->
      <mj-text
        color="#232F3E"
        font-size="16px"
        line-height="24px"
        font-weight="400"
        font-family="Roboto, Arial, Helvetica, sans-serif"
      />
      <mj-image padding="0px" />
      <mj-button
        background-color="#ff9900"
        color="#232f3e"
        font-family="Roboto, Arial, Helvetica, sans-serif"
        border-radius="0px"
        font-size="14px"
        font-weight="700"
        line-height="18px"
        padding="10px 20px"
      />
      <mj-section padding="0" background-color="#FAFAFA" />

      <!-- Custom Classes -->
      <mj-class
        name="head-text"
        font-size="24px"
        line-height="28px"
        font-weight="700"
        align="center"
        font-family="Roboto, Arial, Helvetica, sans-serif"
      />
      <mj-class
        name="section-head-text"
        font-size="18px"
        line-height="22px"
        font-weight="bold"
        font-family="Roboto, Arial, Helvetica, sans-serif"
      />

      <mj-class
        name="reason-text"
        font-size="14px"
        line-height="20px"
        font-weight="600"
        align="center"
        font-family="Roboto, Arial, Helvetica, sans-serif"
      />
    </mj-attributes>
  </mj-head>
  <mj-body>
    <!-- hero image -->
    <mj-section>
      <mj-column>
        <mj-image
          src="https://pages.awscloud.com/rs/112-TZM-766/images/aws-bos-23q1-em-bnr-en.jpg"
          alt="Builders Online Series - Your quick start to AWS"
        />
      </mj-column>
    </mj-section>

    <!-- heading title -->
    <mj-section>
      <mj-column>
        <mj-text mj-class="head-text" padding="30px 0 0 0">
          Jumpstart your cloud journey with AWS
        </mj-text>
      </mj-column>
    </mj-section>

    <!-- details section -->
    <mj-section padding="5px">
      <mj-column>
        <mj-text padding-bottom="15px">
          You are invited to join us on <strong>18 Jan, 2023</strong> for
          <strong>AWS Builders Online Series.</strong> Learn AWS cloud concepts,
          architectural best practices, and step-by-step guides to help you get
          started and accelerate your success on AWS. <br /><br />
          Join<strong> Kris Howard, Head of Dev Relations, APJ, AWS</strong> as
          she shares the <strong>latest updates and launches</strong> from AWS
          re:Invent 2022.<br /><br />
          Whether you are new or simply curious about AWS, join us to learn
          more!
        </mj-text>
        <mj-button href="#"> Register now </mj-button>
      </mj-column>
    </mj-section>

    <!-- Reasons to attend heading section -->
    <mj-section padding="5px 5px 0px 5px">
      <mj-column>
        <mj-text mj-class="section-head-text"> Reasons to attend: </mj-text>
      </mj-column>
    </mj-section>

    <!-- Reasons to attend description section -->
    <mj-section padding="0px 5px 10px 5px">
      <mj-column padding="10px 0">
        <mj-image
          width="100px"
          src="https://pages.awscloud.com/rs/112-TZM-766/images/builders-23q1-icon-opening.png"
          alt=""
        />
        <mj-text mj-class="reason-text"> Live closing & 15 sessions </mj-text>
      </mj-column>
      <mj-column padding="10px 0">
        <mj-image
          width="100px"
          src="https://pages.awscloud.com/rs/112-TZM-766/images/builders-23q1-icon-live.png"
          alt=""
        />
        <mj-text mj-class="reason-text"> Live Q&A with AWS experts </mj-text>
      </mj-column>
      <mj-column padding="10px 0">
        <mj-image
          width="100px"
          src="https://pages.awscloud.com/rs/112-TZM-766/images/builders-23q1-icon-cert.png"
          alt=""
        />
        <mj-text mj-class="reason-text"> Certificate of attendance </mj-text>
      </mj-column>
    </mj-section>

    <!-- Don't miss out these topics heading section -->
    <mj-section padding="5px 5px 0px 5px">
      <mj-column>
