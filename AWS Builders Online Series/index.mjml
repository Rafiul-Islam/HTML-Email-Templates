          alt=""
        />
        <mj-text mj-class="reason-text"> Machine Learning </mj-text>
      </mj-column>
    </mj-section>

    <!-- Event details heading section -->
    <mj-section padding="5px 5px 0px 5px">
      <mj-column>
        <mj-text mj-class="section-head-text"> Event details </mj-text>
      </mj-column>
    </mj-section>

    <!-- Event details heading section -->
    <mj-section padding="5px 0px 5px 30px">
      <mj-column width="50%">
        <mj-text
          font-size="20px"
          font-weight="bold"
          align="center"
