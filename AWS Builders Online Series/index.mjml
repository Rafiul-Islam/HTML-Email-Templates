        <mj-text mj-class="reason-text"> Storage </mj-text>
      </mj-column>
      <mj-column padding="10px 0">
        <mj-image
          width="80px"
          src="https://pages.awscloud.com/rs/112-TZM-766/images/awsome_day_22q2_ico_database.png"
